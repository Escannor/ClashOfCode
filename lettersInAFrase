import sys
import math

# Auto-generated code below aims at helping you parse
# the standard input according to the problem statement.

n = int(input())
s = input()
frase = s.lower()

let = 'a'
while let <= 'z':
    prin = ''
    for i in frase:
        if i == let:
            prin = prin + i#print(i)
    if prin != '':     
        print(prin)
    let = chr( ord(let) + 1)
    
